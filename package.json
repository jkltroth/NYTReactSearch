{
  "name": "nyt-react-search",
  "version": "1.0.0",
  "description": "A React-based New York Times Article Search application that utilizes Node, Express and MongoDB so users can save articles to read later.",
  "main": "server.js",
  "scripts": {
    "server": "node server.js",
    "client": "node scripts/start-client.js",
    "start": "concurrently \"nodemon server.js\" \"npm run client\"",
    "build": "node scripts/build.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "installDeps": "yarn && cd client && yarn"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jkltroth/NYTReactSearch.git"
  },
  "author": "James Roth",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jkltroth/NYTReactSearch/issues"
  },
  "homepage": "https://github.com/jkltroth/NYTReactSearch#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "mongoose": "^5.0.6"
  },
  "devDependencies": {
    "concurrently": "^3.5.1"
  }
}
